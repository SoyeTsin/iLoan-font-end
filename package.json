    "jsencrypt": "^3.0.0-rc.1",
    "mathjs": "^8.0.1",
    "moment": "^2.29.1",
    "qs": "^6.9.4",
    "readline": "^1.3.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-countup-v2": "^4.0.0",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.21.0",
    "vue-router": "^3.2.0",
    "vue-stacked-progress-bar": "^1.0.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
