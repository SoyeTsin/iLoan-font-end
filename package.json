{
  "name": "framing",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "genesis": "vue-cli-service serve --mode genesis",
    "serve-uat": "vue-cli-service serve --mode uat",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --mode pro",
    "deploy-sit": "chmod +x ./scripts/*.sh && chmod 700 ktc-sit && ./scripts/deploy-sit.sh",
    "deploy-uat": "chmod +x ./scripts/*.sh && chmod 700 ktc-sit && ./scripts/deploy-uat.sh",
    "release-all": "git pull && yarn && vue-cli-service build --mode sit && node release.js -mode sit && vue-cli-service build --mode uat && node release.js -mode uat",
    "release-sit": "git pull && yarn && vue-cli-service build --mode sit && node release.js -mode sit",
    "release-uat": "git pull && yarn && vue-cli-service build --mode uat && node release.js -mode uat",
    "release-genesis": "git pull && yarn && vue-cli-service build --mode genesis"
  },
  "dependencies": {
    "ant-design-vue": "^1.6.5",
    "axios": "^0.20.0",
    "ckeditor4-vue": "^1.2.0",
    "commander": "^6.2.0",
